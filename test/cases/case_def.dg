      print 2
      print 3
      print 4
      print 5
    *: foo bar baz:2 -> print 6
    foo bar:1 baz:2 -> print 7
    foo *:bar **:baz -> print 8
    -> print 9
    ->> print 10
    async$ -> print 11
    a -> print 12
    a ->> print 13
